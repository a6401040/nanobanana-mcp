{
  "name": "@nierlu/nanobanana-mcp",
  "version": "1.0.2",
  "description": "MCP server for generating and editing images using Google nanobanana API",
  "main": "dist/server.js",
  "type": "module",
  "bin": {
    "nanobanana-mcp": "dist/server.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node -r dotenv/config dist/server.js",
    "dev": "tsx src/server.ts",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run build && npm test"
  },
  "keywords": [
    "mcp",
    "image-generation",
    "image-editing",
    "modelcontextprotocol",
    "gemini",
    "google-ai",
    "ai-image",
    "nanobanana"
  ],
  "author": "kylin",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a6401040/nanobanana-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/a6401040/nanobanana-mcp.git/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "commander": "^14.0.1",
    "dotenv": "^16.4.5",
    "openai": "^6.9.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
